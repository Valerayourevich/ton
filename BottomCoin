bash
   func -o BottomCoin.fif BottomCoin.fc
fift
   ;; BottomCoin.fif
   ;; Инициализация контракта

   "BottomCoin.fc" include
   <boc> B{ B>constant
fift
   ;; deploy.fif
   ;; Деплой контракта

   "BottomCoin.fif" include
   <boc> B{ B>constant
bash
   fift -s deploy.fif
fift
   ;; interaction.fif
   ;; Взаимодействие с контрактом

   "BottomCoin.fif" include
   <boc> B{ B>constant
bash
   fift -s interaction.fif
